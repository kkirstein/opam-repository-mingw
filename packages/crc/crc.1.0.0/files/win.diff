    Avoid name conflicts in ocaml-crc

diff --git a/lib/crc.ml b/lib/crc.ml
index 41cd282..9291d3b 100644
--- a/lib/crc.ml
+++ b/lib/crc.ml
@@ -31,10 +31,10 @@ let check_bounds_string str offset length =
 	check_bounds (String.length str) offset length
 
 external unsafe_crc32_cstruct : int32 -> Cstruct.buffer -> int -> int -> int32 =
-	"crc32_cstruct"
+	"caml_crc32_cstruct"
 
 external unsafe_crc32_string : int32 -> string -> int -> int -> int32 =
-	"crc32_string"
+	"caml_crc32_string"
 
 module Crc32 = struct
 	let cstruct ?(crc=0l) t =
diff --git a/lib/crc_stubs.c b/lib/crc_stubs.c
index d553e1f..26acf25 100644
--- a/lib/crc_stubs.c
+++ b/lib/crc_stubs.c
@@ -24,7 +24,7 @@
 #include <caml/memory.h>
 #include <caml/signals.h>
 
-static uint32_t crc32_tab[] = {
+static uint32_t caml_crc32_tab[] = {
     0x00000000, 0x77073096, 0xee0e612c, 0x990951ba, 0x076dc419, 0x706af48f,
     0xe963a535, 0x9e6495a3, 0x0edb8832, 0x79dcb8a4, 0xe0d5e91e, 0x97d2d988,
