@@ -71,7 +71,7 @@ static uint32_t crc32_tab[] = {
 };
 
 uint32_t
-crc32(uint32_t crc, const void *buf, size_t size)
+caml_crc32(uint32_t crc, const void *buf, size_t size)
 {
     const uint8_t *p;
 
