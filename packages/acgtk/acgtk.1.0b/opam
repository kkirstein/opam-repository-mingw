maintainer: "sylvain.pogodalla@inria.fr"
build: [
  ["./configure" "--prefix=%{prefix}%"]
  [make "all"]
]
install: [
  [make "install-all"]
]
remove: [
  ["./configure" "--prefix=%{prefix}%"]
  [make "uninstall-all"]
]
